#include<bits/stdc++.h>
using namespace std;
#define ll   long long
#define pb   push_back
#define yes   cout<<"YES"<<endl
#define no   cout<<"NO"<<endl
#define all(x) (x).begin(), (x).end()
#define    gcd(a,b)      __gcd(a,b)
#define    lcm(a,b)  (a*b)/__gcd(a,b)
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

void solve(){

    ll n; 
    cin>>n; 
    ll k=n;
    ll nots[]={ 100, 50, 20, 10, 5, 2, 1};
    for(ll i=0; i<7; i++)
    {
        ll x=n/nots[i];
        cout<<x<<" Nots of "<<nots[i]<<" tk"<<endl;
        n%=nots[i];

    }
    cout<<"TOTAL EID SELAMI "<<k<<" tk"<<endl;
    
}

int main()
{
    optimize();
  
        solve();
    
}
